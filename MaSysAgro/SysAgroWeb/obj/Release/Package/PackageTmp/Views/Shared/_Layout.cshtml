    <style>
        .etiquetaCantidad {
            border-radius: 30%;
            width: 17px;
            text-align: center;
            color: white
        }

        .submenudrop {
            min-width: 18rem;
            padding: 1rem 0;
        }

            .submenudrop .dropdown-header {
                color: #7ab37f;
                font-size: 14px;
                border-bottom: 1px solid #f1f0f0
            }

            .submenudrop .dropdown-item {
                display: block;
                padding: 7px 20px;
                clear: both;
                font-weight: normal;
                line-height: 2.5;
                color: #333;
                white-space: nowrap;
                font-size: 13px
            }

                .submenudrop .dropdown-item.active {
                    color: #fff !important;
                }

                .submenudrop .dropdown-item:hover {
                    color: #fff !important;
                    background-color: #000
                }

        .dropdown-toggle {
            color: rgba(0,0,0,.55);
        }
        .spinner-border {
            background-image: src("~/Content/img/icono-pes.png");
        }
        .preloader {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 9999;
            background-color: #7ab37f;
            opacity: 1;
            text-align: center;
            justify-content: center;
            display: flex;
            align-items: center;
            justify-content: center;
        }
       /* preloader img {
            width: 80px;
            -webkit-animation: spin .2s linear infinite;
            -moz-animation: spin .2s linear infinite;
            animation: spin .2s linear infinite;
        }*/
        .spinner-border {
            --bs-spinner-width: 2rem;
            --bs-spinner-height: 2rem;
            --bs-spinner-vertical-align: -0.125em;
            --bs-spinner-border-width: 0.25em;
            --bs-spinner-animation-speed: 0.75s !important;
            --bs-spinner-animation-name: spinner-border;
            border: var(--bs-spinner-border-width) solid currentcolor;
            border-right-color: transparent;
            animation-duration: 1.3s!important;
        }
    </style>
