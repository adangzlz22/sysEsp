<style>
    .file-upload {
        background-color: #ffffff;
        width: 100%;
        margin: 0 auto;
        padding: 8px;
    }

        .file-upload:hover {
            color: #ffffff !important;
        }

    .file-upload-content {
        display: none;
        text-align: center;
    }

    .file-upload-input {
        position: absolute;
        margin: 0;
        padding: 0;
        width: 100%;
        height: 100%;
        outline: none;
        opacity: 0;
        cursor: pointer;
    }

    .image-upload-wrap {
        border: 1px dashed #7ab37f;
        position: relative;
        background-color: #7ab37f1a;
        color: white;
        border-radius: 10px;
    }

        .image-dropping,
        .image-upload-wrap:hover {
            background-color: #7ab37f1a;
            border: 1px dashed #7ab37f;
            border-radius: 10px
        }

    .image-title-wrap {
        padding: 0 15px 15px 15px;
        color: #222;
    }

