        text-align: center;
    }

        .drag-text h5 {
            font-weight: 100;
        }

            .drag-text h5:hover {
                font-weight: 100;
                color: #fff;
            }

    .file-upload-image {
        max-height: 400px;
        max-width: 200px;
        margin: auto;
        padding: 5px;
    }

    .remove-image {
        width: 100%;
        margin: 0;
        color: #ff0000;
        background: none;
        border: none;
        padding: 10px;
        border-radius: 4px;
        transition: all .2s ease;
        outline: none;
    }

        .remove-image:hover {
            background: none;
            color: #000;
            transition: all .2s ease;
            cursor: pointer;
        }

        .remove-image:active {
            border: 0;
            transition: all .2s ease;
        }

    .box {
        box-shadow: 0 0.1rem 1rem 0.25rem rgba(0,0,0,.05)
    }

        .box:hover {
            box-shadow: 0 0 11px rgb(122 179 127 / 58%);
